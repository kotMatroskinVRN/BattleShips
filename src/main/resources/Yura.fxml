            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="home.battleShips.Controller" stylesheets="Yura.css">

   <center>
      <SplitPane dividerPositions="0.5" prefHeight="160.0" prefWidth="200.0" visible="true" BorderPane.alignment="CENTER">
        <items>
            <BorderPane fx:id="playerPane">
            <bottom>

            </bottom>
            </BorderPane>
          <BorderPane fx:id="computerPane">

          </BorderPane>
        </items>
      </SplitPane>
   </center>
   <bottom>
      <Button fx:id="newGame" mnemonicParsing="false" text="Новая Игра" BorderPane.alignment="CENTER"
              onAction="#newGame">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin></Button>
   </bottom>
</BorderPane>
